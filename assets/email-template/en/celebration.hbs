{{#> layouts/base }}

{{#*inline "hero-block"}}
<tr>
    <td valign="top" class="side title"
        style="border-collapse: collapse;border: 0;margin: 0;padding: 20px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;vertical-align: top;background-color: white;border-top: none; padding-bottom: 50px;">
        <table
            style="font-weight: normal;border-collapse: collapse;border: 0;margin: 0;padding: 0;font-family: Arial, sans-serif;">
            <tr>
                <td class="sub-title"
                    style="border-collapse: collapse;border: 0;margin: 0;padding: 0;padding-top:5px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 18px;line-height: 29px;font-weight: bold;text-align: center;">
                    <div class="mktEditable" id="intro_title">
                        <!-- Sub title -->
                    </div>
                </td>
            </tr>
            <tr>
                <td class="grey-block"
                    style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align:center;">
                    <div class="mktEditable" id="cta">
                        <img class="sub-image" style="max-height: 319px; width: 100%;" src={{img}} alt="img" /><br><br>
                    </div>
                </td>
            </tr>
            <tr>
                <td class="head-title"
                    style="border-collapse: collapse;border: 0;margin: 0;padding: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 28px;line-height: 34px;font-weight: bold; text-align: center;padding-top: 1px;">
                    <div class="mktEditable" id="main_title">{{title}}</div>
                </td>
            </tr>
            <tr>
                <td class="text"
                    style="border-collapse: collapse;border: 0;margin: 0;padding: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px; text-align: center;padding-top: 20px;">
                    <div class="mktEditable" id="main_text">{{description}}</div>
                </td>
            </tr>
            <tr>
                <td class="text" style="padding-top: 20px; text-align: center;">
                    <a style="color:#ffffff; background-color: #31154E;  border-top: 10px solid #31154E; border-bottom: 10px solid #31154E; border-left: 20px solid #31154E; border-right: 20px solid #31154E; border-radius: 3px; text-decoration:none;"
                        href="https://darkkom.ma">Visit Our Site</a>
                </td>
            </tr>
        </table>
    </td>
</tr>{{/inline}}

{{/layouts/base}}