{{#> layouts/base }}
{{#*inline "hero-block"}}
<tr>
  <td valign="top" class="side title"
    style="border-collapse: collapse;border: 0;margin: 0;padding: 20px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;vertical-align: top;background-color: white;border-top: none; padding-bottom: 50px;">
    <table
      style="font-weight: normal;border-collapse: collapse;border: 0;margin: 0;padding: 0;font-family: Arial, sans-serif;">
      <tr>
        <td class="sub-title"
          style="border-collapse: collapse;border: 0;margin: 0;padding: 0;padding-top:5px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 18px;line-height: 29px;font-weight: bold;text-align: center;">
          <div class="mktEditable" id="intro_title">
            Subscription is about to expire
          </div>
        </td>
      </tr>
      <tr>
        <td class="head-title"
          style="border-collapse: collapse;border: 0;margin: 0;padding: 20px; padding-top: 20px;padding-bottom: 10px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 14px;line-height: 34px;font-weight: bold; text-align: center;">
          <div class="mktEditable" id="main_title">{{msg}}<br>
            Thank you for using your Darkkom platform. We always strive to provide everything you need and we are working on adding a lot of features
            If you have any questions, you can contact us, and do not forget, dear customer, you see that we care.
          </div>
        </td>
      </tr>
    </table>
  </td>
</tr>
{{/inline}}
{{/layouts/base}}